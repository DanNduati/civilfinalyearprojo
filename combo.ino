#include <DallasTemperature.h>
#include <OneWire.h>
#include <SPI.h>
#include <SD.h>
#include "RTClib.h"
#define oneWireBus 2
#define buttonPin 3

//debugging leds
#define led1 9
#define led2 8
int ledState = LOW; //log status led
const long interval = 200;//blink interval
unsigned long previousMillis = 0;
//sd card and rtc shield
const int chipSelect = 10;
RTC_DS1307 RTC;
//temperature sensors
float temperature;
OneWire  ds(oneWireBus);  //create onewire object to enable one wire comm with the temperature sensor
DallasTemperature sensors(&ds);
int deviceCount = 0;
volatile int sampleCount;
File dataFile;
DateTime now;

void setup(void) {

  Serial.begin(9600);
  // use debugging LEDs
  pinMode(led1, OUTPUT);
  pinMode(led2, OUTPUT);
  //get temp sensor count
  deviceCount = sensors.getDeviceCount();
  //setup clock
  RTC.begin();
  //check or the Real Time Clock is on
  if (! RTC.isrunning()) {
    Serial.println("RTC is NOT running!");
    //Set date and time
    RTC.adjust(DateTime(__DATE__, __TIME__));
  }
  Serial.print("Initializing SD card...");
  // check if the SD card is present and can be initialized:
  if (!SD.begin(chipSelect)) {
    error("card failed or not present");
    // don't do anything more:
    return;
  }
  Serial.println("card initialized.");

  writeLogData();
}

void loop(void) {

  writeSensorData();
  delay();
}

float getTempIndex(int sensor) {
  sensors.requestTemperatures();
  temperature = sensors.getTempCByIndex(sensor);
  return temperature;
}

void writeSensorData() {
  now = RTC.now();

  //open file to log data in.
  dataFile = SD.open("DATA.CSV", FILE_WRITE);
  //printTemp();
  // if the file is available, write to it:
  // log the temperatures and time.
  if (dataFile) {
    dataFile.print(getTempIndex(0));
    dataFile.print(", ");
    dataFile.print(getTempIndex(1));
    dataFile.print(", ");
    dataFile.print(getTempIndex(2));
    dataFile.print(", ");
    dataFile.print(getTempIndex(3));
    dataFile.print(", ");
    dataFile.print(getTempIndex(4));
    dataFile.print(", ");
    dataFile.print(now.hour(), DEC);
    dataFile.print(":");
    dataFile.print(now.minute(), DEC);
    dataFile.print(":");
    dataFile.print(now.second(), DEC);
    dataFile.print(" ");
    dataFile.print(now.year(), DEC);
    dataFile.print('/');
    dataFile.print(now.month(), DEC);
    dataFile.print('/');
    dataFile.println(now.day(), DEC);

    dataFile.close();
    // print to the serial port too:
    Serial.println("data stored");
    blinkLog();
  }
  // if the file isn't open, pop up an error:
  else {
    error("could not open datalog file");
  }
}

void writeLogData() {
  //write down the date (year / month / day) to the datalog txt file
  now = RTC.now();
  dataFile = SD.open("DATA.CSV", FILE_WRITE);
  dataFile.print("sensor1,sensor2,sensor3,sensor4,sensor5,DateTime");
  dataFile.println();
  dataFile.close();
}

void printTemp() {
  for (int i = 0; i < deviceCount; i++) {
    Serial.print(getTempIndex(i));
    Serial.print(" ");
  }git 
  Serial.println(" ");
  //delay(1000);
}

void error(char *str) {
  Serial.print("error: ");
  Serial.println(str);

  // red LED indicates error
  digitalWrite(led1, HIGH);
  //do not do shit
  while (1);
}
//fuction to blink log status led
void blinkLog() {
  unsigned long currentMillis = millis();
  if (currentMillis - previousMillis >= interval) {
    previousMillis = currentMillis;
    //toggle
    if (ledState == LOW) {
      ledState = HIGH;
    } else {
      ledState = LOW;
    }
    digitalWrite(led2, ledState);
  }
}
